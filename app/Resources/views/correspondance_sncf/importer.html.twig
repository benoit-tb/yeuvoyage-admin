{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col-lg-12">
            <div class="box">
                <div class="box-header">
                    <h2><i class="fa fa-edit"></i>Importer des horaires</h2>
                    <div class="box-icon">
                        <a class="btn-setting" href="form-dropzone.html#"><i class="fa fa-wrench"></i></a>
                        <a class="btn-minimize" href="form-dropzone.html#"><i class="fa fa-chevron-up"></i></a>
                        <a class="btn-close" href="form-dropzone.html#"><i class="fa fa-times"></i></a>
                    </div>
                </div>
                <div class="box-content">
                    <div class="box-content search-form">
                        <form class="form-inline" method="POST" {{ form_enctype(form) }}>
                            {{ form(form) }}
                        </form>
                    </div>
                </div>
            </div>
        </div><!--/col-->
    </div>
{% endblock %}